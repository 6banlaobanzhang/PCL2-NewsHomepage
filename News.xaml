					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="添加 thrown_item_picked_up_by_player 进度触发器。" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="添加 avoid_vibration 进度触发器。" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="添加 sonic_explosion 粒子效果。" />
				</StackPanel>
				<TextBlock Text="游戏事件" Style="{StaticResource h4}" />
				<StackPanel Style="{StaticResource ListStyle}" Margin="60,10,0,10">

<StackPanel Margin="0,-10,0,0">
<!--Animations Starts-->
<StackPanel.Triggers>
    <EventTrigger RoutedEvent="StackPanel.Loaded">
        <BeginStoryboard>
			<Storyboard>
				<DoubleAnimation
					Storyboard.TargetName="verBorder"
					Storyboard.TargetProperty="Width"
					From="0" To="0" Duration="0:0:0.1"/>
				<DoubleAnimation BeginTime="0:0:0.1"
					Storyboard.TargetName="verBorder"
					Storyboard.TargetProperty="Width"
					From="0" To="190" Duration="0:0:0.15"/>
				<DoubleAnimation BeginTime="0:0:0.5"
					Storyboard.TargetName="verBorder"
					Storyboard.TargetProperty="Width"
					From="190" To="160" Duration="0:0:0.1"/>
			</Storyboard>
        </BeginStoryboard>
    </EventTrigger>
    <EventTrigger RoutedEvent="StackPanel.Loaded" >
           <BeginStoryboard>
			<Storyboard x:Name="animation1" RepeatBehavior="Forever" >
				<ColorAnimation
					Storyboard.TargetName="NewsHint"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					From="#FF000000" To="#FF000000" Duration="0:0:0"/>
				<ColorAnimation BeginTime="0:0:5"
					Storyboard.TargetName="NewsHint"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					From="#FF000000" To="#00000000" Duration="0:0:1"/>
				<ColorAnimation BeginTime="0:0:13"
					Storyboard.TargetName="NewsHint"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					From="#00000000" To="#FF000000" Duration="0:0:1"/>
				<ColorAnimation
					Storyboard.TargetName="Hint2"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					From="#00000000" To="#00000000" Duration="0:0:0"/>
				<ColorAnimation BeginTime="0:0:6"
					Storyboard.TargetName="Hint2"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					From="#00000000" To="#FF000000" Duration="0:0:1"/>
				<ColorAnimation BeginTime="0:0:12"
					Storyboard.TargetName="Hint2"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					From="#FF000000" To="#00000000" Duration="0:0:1"/>
			</Storyboard>
        </BeginStoryboard>
    </EventTrigger>
	<EventTrigger RoutedEvent="StackPanel.Loaded" >
           <BeginStoryboard>
			<Storyboard x:Name="animation202141" RepeatBehavior="Forever" >
				<ColorAnimation BeginTime="0:0:0"
					Storyboard.TargetName="MinecraftPlus"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					From="#FFEE9999" To="#FF99EE99" Duration="0:0:1"/>
				<ColorAnimation BeginTime="0:0:1"
					Storyboard.TargetName="MinecraftPlus"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					From="#FF99EE99" To="#FF9999EE" Duration="0:0:1"/>
				<ColorAnimation BeginTime="0:0:2"
					Storyboard.TargetName="MinecraftPlus"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					From="#FF9999EE" To="#FFEE9999" Duration="0:0:1"/>
			</Storyboard>
        </BeginStoryboard>
    </EventTrigger>
</StackPanel.Triggers>
<!--Animations Ends-->

<StackPanel.Resources>
<!--Template Starts-->
	
<!--Template Ends-->

<!--Styles Starts-->
    <Style TargetType="Ellipse">
        <Setter Property="Fill" Value="{DynamicResource ColorBrush3}"/>
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="Height" Value="8" />
		<Setter Property="Width" Value="8" />
		<Setter Property="Margin" Value="0,0,10,0" />
    </Style>
	<Style TargetType="TextBlock">
        <Setter Property="TextWrapping" Value="Wrap"/>
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="FontSize" Value="16" />
		<Setter Property="Foreground" Value="Black"/>
    </Style>
	<Style x:Key="ListItem" TargetType="TextBlock">
		<Setter Property="Margin" Value="15,-15,5,15"/>
        <Setter Property="TextWrapping" Value="Wrap"/>
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="FontSize" Value="16" />
		<Setter Property="Foreground" Value="Black"/>
    </Style>
	<Style x:Key="ListStyle" TargetType="StackPanel">
        <Setter Property="Margin" Value="45,0,0,5"/>
    </Style>
	<Style x:Key="Read" TargetType="local:MyButton">
        <Setter Property="Margin" Value="0,0,10,0"/>
        <Setter Property="Height" Value="35"/>
		<Setter Property="Padding" Value="13,0,13,0"/>
		<Setter Property="EventType" Value="打开网页"/>
    </Style>
		<Style x:Key="Read0" TargetType="local:MyButton" BasedOn="{StaticResource Read}">
			<Setter Property="Grid.Column" Value="0"/>
			<Setter Property="Text" Value="Minecraft Wiki"/>
			<!--<Setter Property="ColorType" Value="Highlight"/>-->
		</Style>
		<Style x:Key="Read1" TargetType="local:MyButton" BasedOn="{StaticResource Read}">
			<Setter Property="Grid.Column" Value="1"/>
			<Setter Property="Text" Value="MCBBS"/>
		</Style>
		<Style x:Key="Read2" TargetType="local:MyButton" BasedOn="{StaticResource Read}">
			<Setter Property="Grid.Column" Value="2"/>
			<Setter Property="Text" Value="官网"/>
		</Style>
	<!--[W-I-P]
		<Style x:Key="LogGrid" TargetType="Grid">
		<Setter Property="UseLayoutRounding" Value="True"/>
		<Setter Property="Margin" Value="10,10,10,10"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="Grid">
					<ControlTemplate.ColumnDefinitions>
						<ControlTemplate Width="20*"/>
						<ControlTemplate Width="10*"/>
						<ControlTemplate Width="10*"/>
					</ControlTemplate.ColumnDefinitions>
				</ControlTemplate>
			</Setter.Value>
		</Style>-->
	<Style x:Key="Code" TargetType="Border">
        <Setter Property="Background" Value="#e0e0e0"/>
        <Setter Property="BorderBrush" Value="#444"/>
		<Setter Property="CornerRadius" Value="1"/>
		<Setter Property="Margin" Value="5,0,5,0"/>
		<Setter Property="BorderThickness" Value="1"/>
    </Style>
	<Style TargetType="TextBox">
		<Setter Property="Background" Value="{x:Null}"/>
		<Setter Property="Padding" Value="3,0,3,0"/>
		<Setter Property="IsReadOnly" Value="True"/>
		<Setter Property="BorderBrush" Value="{x:Null}"/>
	</Style>
	<Style x:Key="verImgBdr" TargetType="Border">
		<Setter Property="HorizontalAlignment" Value="Center"/>
		<Setter Property="BorderThickness" Value="4"/>
		<Setter Property="VerticalAlignment" Value="Top"/>
		<Setter Property="BorderBrush" Value="{DynamicResource ColorBrush3}"/>
		<Setter Property="CornerRadius" Value="7"/>
	</Style>
	<Style x:Key="verTxtBlk" TargetType="TextBlock">
		<Setter Property="HorizontalAlignment" Value="Center"/>
		<Setter Property="TextAlignment" Value="Center"/>
		<Setter Property="VerticalAlignment" Value="Center"/>
		<Setter Property="Foreground" Value="{DynamicResource ColorBrush7}"/>
		<Setter Property="Width" Value="160"/>
		<Setter Property="TextWrapping" Value="Wrap"/>
		<Setter Property="FontSize" Value="28"/>
	</Style>
	<Style x:Key="verTxtBdr" TargetType="Border">
		<Setter Property="Margin" Value="0,-22,-1,0"/>
		<Setter Property="Background" Value="{DynamicResource ColorBrush3}"/>
		<Setter Property="Width" Value="160"/>
		<Setter Property="Height" Value="40"/>
		<Setter Property="CornerRadius" Value="5"/>
	</Style>

</StackPanel.Resources>
<!--Styles Ends-->

<!--Hints Starts-->
<Border Margin="0,0,0,10" Height="30" Padding="2" BorderThickness="1" Background="{DynamicResource ColorBrush5}" CornerRadius="5" VerticalAlignment="Top" BorderBrush="{DynamicResource ColorBrush3}" Opacity="0.7">

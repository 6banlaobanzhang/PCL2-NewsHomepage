					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="重命名物品标签 occludes_vibration_signals 为 dampens_vibrations 物品标签。" />
				</StackPanel>

			</StackPanel>
			<TextBlock Text="该版本修复了 50 个bug，本文来源于 MCBBS" Style="{StaticResource footnote}" />
			<!--Logs-->

<local:MyCard x:Name="TOPNEW" Margin="0,0,0,15" Title="最新快照 - 2‏‍‌‎‌‎1w15a" CanSwap="True">
	<!--Header-->
	<StackPanel Margin="30,40,30,20">
		<Border Style="{StaticResource verImgBdr}">
            <Border.Background>
                <ImageBrush ImageSource="https://z3.ax1x.com/2021/04/15/cgFFoR.jpg" Stretch="Uniform"/>
            </Border.Background>
            <Image Source="https://z3.ax1x.com/2021/04/15/cgFFoR.jpg" Opacity="0"/>
        </Border>
		<Border x:Name="verBorder" Style="{StaticResource verTxtBdr}">
			<TextBlock Style="{StaticResource verTxtBlk}" Text="21W15A"/>
		</Border>
		
	<!--Detail-->
	<TextBlock Margin="30,3,10,3" Text="注意：由于世界范围再更改，该版本不能加载 21w06a 至 21w14a 的世界。" />
	
	<TextBlock Text="世界生成" FontSize="18" Padding="30,0,0,0" Margin="0,10,0,15"/>
		<StackPanel Style="{StaticResource ListStyle}">
		
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="世界高度限制重新变回 1.16。"/>
		
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="关闭了 1.17 更新以来的全部的洞穴、裂隙、含水层生成。"/>
		
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="矿石分布调整还原至 1.16。"/>
		
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="铜生成于y轴 0-192，y=96 时最常见。"/>
		
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="凝灰岩在y轴 0-16 以团簇分布。"/>
		</StackPanel>
		
	
	<TextBlock Text="原型数据包" FontSize="18" Padding="30,0,0,0" Margin="0,10,0,15"/>
	
		<StackPanel Style="{StaticResource ListStyle}">
		
		<TextBlock Text="你可以使用官方提供的数据包来尝试后期的地形生成。"/>
		<local:MyButton Height="35" Margin="80,20" Text="点击下载" EventType="下载文件" EventData="https://launcher.mojang.com/v1/objects/643b1f8f7f71c74ffc913e8572f52fd5bca88282/CavesAndCliffsPreview.zip" ToolTip="下载洞穴与山崖原型数据包"/>
		
		<TextBlock Text="注意：数据包不能被添加至已创建的世界。使用数据包创建的世界会被标记为实验性世界，这些世界不保证能在之后的版本中继续使用。"/>
		
        </StackPanel>

	<TextBlock Text="新内容" FontSize="18" Padding="30,0,0,0" Margin="0,10,0,15"/>
	
		<StackPanel Style="{StaticResource ListStyle}">	
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="山羊会冲撞所有移动的东西（包括盔甲架）。" />
		</StackPanel>
		
	<TextBlock Text="更改" FontSize="18" Padding="30,0,0,0" Margin="0,10,0,15"/>
	
		<StackPanel Style="{StaticResource ListStyle}">	
		
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="创造模式物品栏建筑方块选项卡顺序重排。" />
		
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="更新了主菜单界面背景。" />
		
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="苦力怕知道了不应该惹怒山羊。" />
		
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="粗矿材质调整。" />
		
		<Image Source="https://z3.ax1x.com/2021/04/15/cgFAF1.png" Margin="10,0,10,0"/>
		<TextBlock Text="材质调整对比图" FontSize="14" Foreground="#FF777777" HorizontalAlignment="Center" Margin="0,0,0,15"/>
		
		<Ellipse/>
		<TextBlock Style="{StaticResource ListItem}" Text="可以合成粗矿方块以节省背包空间了。" />
		
		
		</StackPanel>
		
	<TextBlock Text="技术性更改" FontSize="18" Padding="30,0,0,0" Margin="0,10,0,15"/>
	
		<StackPanel Style="{StaticResource ListStyle}">	
		

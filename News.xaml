			<local:MyButton Style="{StaticResource Read1}" EventData="https://www.mcbbs.net/thread-1191583-1-1.html"/>
			<local:MyButton Style="{StaticResource Read2}" EventData="https://www.minecraft.net/zh-hans/article/minecraft-snapshot-21w15a" />
		</Grid>
	</GroupBox>
	</StackPanel>
